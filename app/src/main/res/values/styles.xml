        <item name="cardBackgroundColor">@color/Tawa</item>
        <item name="strokeColor">@color/Paarha</item>
        <item name="strokeWidth">0.5dp</item>
    </style>


    <style name="SakKantoni">
        <item name="android:scrollbarAlwaysDrawVerticalTrack">true</item>
        <item name="android:scrollbarStyle">insideOverlay</item>
        <item name="android:scrollbars">vertical</item>
        <item name="android:fadeScrollbars">true</item>
        <item name="android:scrollbarThumbVertical">@drawable/sak_kantoni</item>
        <item name="android:scrollbarSize">4dp</item>
        <item name="cornerRadius">24dp</item>
    </style>
    <style name="SakKsaka">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@drawable/sak_ksaka</item>
    </style>


    <style name="Tahaq">
