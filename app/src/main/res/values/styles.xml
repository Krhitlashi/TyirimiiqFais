        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">@dimen/CatsiinaCiihiiMii</item>
        <item name="cornerSizeTopLeft">@dimen/CatsiinaCiihii</item>
        <item name="cornerSizeBottomRight">@dimen/CatsiinaCiihii</item>
        <item name="cornerSizeBottomLeft">@dimen/CatsiinaCiihiiMii</item>
