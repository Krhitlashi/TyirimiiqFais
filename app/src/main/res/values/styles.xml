        <item name="cornerSizeBottomRight">@dimen/Catsiina</item>
        <item name="cornerSizeBottomLeft">@dimen/CatsiinaMii</item>
        <item name="android:shadowColor">@android:color/transparent</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:elevation">0dp</item>
        <item name="android:padding">8dp</item>
        <item name="cardBackgroundColor">@color/Tiisha</item>
        <item name="strokeColor">@color/Tawa</item>
